/**index.wxss**/
view {
  /* 适用主体颜色 */
  /* color: var(--themeColor); */
}

.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}

.index_floor .floor_group .floor_title image {
  width: 100%;
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: calc(100% / 3);
}
/* 后四个超链接 */
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /* 第一张图片的高度 height: calc(33.33vw * 386 / 232); */
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_list navigator:nth-child(2), .index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}